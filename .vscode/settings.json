    "code-runner.runInTerminal": true,
    "C_Cpp.default.compilerPath": "",
    "docwriter.style": "Auto-detect",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 4,
    "editor.wordWrap": "on",
    "editor.formatOnSave": true,
    "prettier.tabWidth": 4
